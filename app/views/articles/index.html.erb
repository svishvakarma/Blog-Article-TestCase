<h1>Article index</h1>

<table class="article-table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Body</th>
    </tr>
  </thead>
  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td><%= article.name %></td>
        <td><%= article.body %></td>
      </tr>
    <% end %>
  </tbody>
</table>

			
