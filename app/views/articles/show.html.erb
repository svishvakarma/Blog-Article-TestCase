<h1>Article show</h1>

<table class="article-table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Body</th>
      <th>New</th>
      <th>Edit</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @article.name %></td>
      <td><%= @article.body %></td>
      <td><%= link_to "New Article", new_article_path %></td>
      <td><%= link_to "Edit Article", edit_article_path(id: @article.id) %></td>
    </tr>
  </tbody>
</table>
